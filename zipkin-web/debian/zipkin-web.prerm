#!/bin/sh
set -e
#
# Adding service to autostart
# $1 = service name
#
startService() {
    app_name=$1
    initctl reload-configuration
    service $app_name start
}

#
# Removing service from autostart
# $1 = service name
#
stopService() {
    app_name=$1
    service $app_name stop
}


stopService zipkin-web || echo "zipkin-web wasn't even running!"
